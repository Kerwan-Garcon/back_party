{
    "version":2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/.*",
            "dest": "src/main.ts",
            "methods": ["GET","POST","PUT","DELETE"]
        }
    ],
    "build": {
        "env": {
          "PRISMA_QUERY_ENGINE_BINARY": "./node_modules/.bin/query-engine"
        },
        "postinstall": "prisma generate"
      }
}